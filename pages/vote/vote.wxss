page {
  font-family: 'Microsoft YaHei';
  font-size: 28rpx;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background-color: #eee;
}

.container {
  padding: 20rpx;
  background-color: #fff;
}

image {
  width: 100%;
  margin-bottom: 20rpx;
}

.title {
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.content {
  line-height:35rpx;
  text-indent: 20px;
  font-size: 30rpx;
  text-align: justify;
}

.votes {
  padding-bottom: 100rpx;
}

.v-box {
  padding: 20rpx;
  background-color: #fff;
  margin: 10rpx 0;
  height: 70rpx;
  line-height: 70rpx;
}

.radio:before {
  content: '';
  display: inline-block;
  vertical-align: middle;
  width: 50rpx;
  height: 50rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
  margin-right: 20rpx;
}

.radioed:before {
  content: '';
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
  vertical-align: middle;
  margin-right: 20rpx;
}

.button {
  position: fixed;
  left: 0;
  bottom: 0;
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  font-size: 40rpx;
  background-color: #1aad19;
  color: #fff;
  border-radius: 10rpx;
}
.voted .v-box view{
  display: inline-block;
}
.choose{
  width: 40%;
}
.num{
  width: 10%;
}
.process{
  width: 50%;
  display: flex;
  justify-content: space-between;
}
.p-detail{
  display: inline-block;
  width: 40%;
  height: 20rpx;
  background-color: #1aad19;
}